{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "monorepo": false,
  "root": ".",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "tsconfig.app.json"
  },
  "projects": {
    "service-customer": {
      "type": "application",
      "root": ".",
      "entryFile": "main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.app.json"
      }
    }
  }
}
